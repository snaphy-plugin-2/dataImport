{
  "name": "dataImport",
  "description": "Plugin to import data from excel into database",
  "routeExposure": "/dataImport",
  "activate": true,
  "pluginDependencies": {},
  "constants":{
    "sheetColNumber": "sheetColNumber"
  },
  "upload":{
    "containerModel": "ImportData",
    "fileDataSource": "ImportData"
  },
  "filePath":{
    "beforeSave": "../../synapsica/backend/beforeSave.js"
  },
  "worksheet":{
    "Sheet1":{
      "structure":[
        {
          "model": "Degree",
          "properties":[
            {
              "sheetColName": "name",
              "modelProperty":"name",
              "unique": true
            }
          ],
          "beforeSave":["addDegree"]
        }
      ]
    }
  }
}
